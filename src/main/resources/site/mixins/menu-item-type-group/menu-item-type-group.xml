<?xml version="1.0" encoding="UTF-8"?>
<mixin>
	<display-name>Menu item</display-name>
	<items>
		<option-set name="option">
			<label>Item</label>
			<expanded>false</expanded>
			<occurrences minimum="1" maximum="0"/>
			<help-text></help-text>
			<options minimum="1" maximum="1">
				<option name="content">
					<label>Content</label>
					<help-text></help-text>
					<default></default>
					<items>
						<inline mixin="menu-item-type-content"/>
					</items>
				</option>
				<option name="url">
					<label>Url</label>
					<help-text></help-text>
					<items>
						<inline mixin="menu-item-type-url"/>
					</items>
				</option>
				<option name="placeholder">
					<label>Placeholder</label>
					<help-text></help-text>
					<default></default>
					<items>
						<inline mixin="menu-item-type-placeholder"/>
					</items>
				</option>
				<option name="divider">
					<label>Divider</label>
					<help-text></help-text>
					<default></default>
				</option>
			</options>
		</option-set>
	</items>
</mixin>
