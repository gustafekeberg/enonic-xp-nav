<?xml version="1.0" encoding="UTF-8"?>
<mixin>
	<display-name>Menu item type url</display-name>
	<items>
		<input name="title" type="TextLine">
			<label>Title</label>
			<help-text></help-text>
			<occurrences minimum="1" maximum="1"/>
			<config>
				<regexp></regexp>
			</config>
			<default></default>
		</input>
		<input name="url" type="TextLine">
			<label>Url</label>
			<help-text></help-text>
			<occurrences minimum="1" maximum="1"/>
			<config>
				<regexp></regexp>
			</config>
			<default></default>
		</input>
		<inline mixin="menu-item-target"/>
	</items>
</mixin>
